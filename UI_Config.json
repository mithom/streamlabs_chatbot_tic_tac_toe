{
  "output_file":"tictactoeSettings.json",
  "play_command":{
    "type":"textbox",
    "value":"!play",
    "label":"play command",
    "tooltip":"command to play a turn",
    "group":"commands & responses"
  },
  "start_command":{
    "type":"textbox",
    "value":"!tictactoe",
    "label":"start command",
    "tooltip":"command to challenge/accept a game",
    "group":"commands & responses"
  },
  "currency_name":{
    "type":"textbox",
    "value":"points",
    "label":"currency name",
    "tooltip":"name of the used currency",
    "group":"commands & responses"
  },
  "start_cost":{
    "type":"numberbox",
    "value":1,
    "label":"start cost",
    "tooltip":"cost for starting a game, only charged when accepted",
    "group":"costs & rewards"
  },
  "win_reward":{
    "type":"numberbox",
    "value":2,
    "label":"win reward",
    "tooltip":"reward for winning a game",
    "group":"costs & rewards"
  },
  "challenge_time":{
    "type":"numberbox",
    "value":30,
    "label":"challenge time",
    "tooltip":"time to accept a challenge",
    "group":"config"
  },
  "spam_chat":{
    "type":"checkbox",
    "value": false,
    "label":"print playfield in chat (not recommended)",
    "tooltip":"best only used for testing, really spammy",
    "group":"config"
  },
  "border_thickness":{
    "type":"numberbox",
    "value":3,
    "label":"border thickness (px)",
    "tooltip":"thickness of the lines dividing the playfield",
    "group":"view"
  },
  "field_color":{
    "type":"colorpicker",
    "value":"rgba(255,255,255,255)",
    "label":"field color",
    "tooltip":"background color of the playing field",
    "group":"view"
  },
  "border_color":{
    "type":"colorpicker",
    "value":"rgba(0,0,0,255)",
    "label":"border color",
    "tooltip":"border color of the playing field",
    "group":"view"
  }
}